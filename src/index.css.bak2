.service-card-elevated:hover {
    box-shadow:
        0 5px 15px rgba(74, 93, 115, 0.1),
        0 20px 50px rgba(74, 93, 115, 0.15);
}

/* CTA Section Enhancement */
.cta-section {
    background: linear-gradient(135deg, var(--footer-bg-start) 0%, var(--footer-bg-end) 100%);
    position: relative;
    overflow: hidden;
}

.cta-section::before {
    content: '';
    position: absolute;
    top: -40%;
    left: -5%;
