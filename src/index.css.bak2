.cta-section::after {
    content: '';
    position: absolute;
    bottom: -40%;
    right: -5%;
    width: 500px;
    height: 500px;
    background: radial-gradient(circle, rgba(74, 93, 115, 0.12) 0%, transparent 70%);
    border-radius: 50%;
}

.cta-section h2 {
    color: white;
    font-size: 3rem;
    margin-bottom: 1rem;
}

.cta-section .btn-primary {
    background: var(--gradient-primary);
}

.cta-section .btn-outline {
    border-color: rgba(255, 255, 255, 0.4);
    color: white;
}

.cta-section .btn-outline:hover {
    background: rgba(255, 255, 255, 0.15);
    border-color: white;
}

/* ==================================
   SITE FOOTER
   ================================== */

.site-footer {
    background: linear-gradient(135deg, var(--footer-bg-start) 0%, var(--footer-bg-end) 100%);
    color: var(--footer-text);
