/* ==================================
   PREMIUM WEBSITE ENHANCEMENTS
   ================================== */

/* Enhanced Service Cards */
.service-card-minimal {
    background: var(--bg-white);
    padding: 2.5rem 2rem;
    border-radius: 16px;
    border: 1px solid rgba(0, 0, 0, 0.06);
    cursor: pointer;
    transition: all 0.4s cubic-bezier(0.25, 1, 0.5, 1);
    position: relative;
    overflow: hidden;
}

.service-card-minimal::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    height: 3px;
    background: linear-gradient(90deg, var(--accent-it) 0%, #4A5D73 100%);
    transform: scaleX(0);
    transform-origin: left;
    transition: transform 0.4s ease;
}

.service-card-minimal:hover::before {
    transform: scaleX(1);
}

.service-card-minimal:hover {
    transform: translateY(-8px);
    box-shadow: 0 20px 50px rgba(74, 93, 115, 0.15);
    border-color: rgba(74, 93, 115, 0.3);
}

.service-card-minimal h3 {
    font-size: 1.35rem;
    font-weight: 600;
    margin-bottom: 0.75rem;
    line-height: 1.3;
    color: var(--text-primary);
    transition: color 0.3s ease;
}

.service-card-minimal:hover h3 {
    color: var(--accent-it);
}

.service-card-minimal p {
    font-size: 0.95rem;
    line-height: 1.6;
    color: var(--text-secondary);
    margin-bottom: 1.5rem;
}

.card-arrow {
    display: flex;
    align-items: center;
    justify-content: flex-end;
    color: var(--accent-it);
    opacity: 0;
    transform: translateX(-10px);
    transition: all 0.3s ease;
}

.service-card-minimal:hover .card-arrow {
    opacity: 1;
    transform: translateX(0);
}

.service-cards-grid {
    display: grid;
    grid-template-columns: repeat(3, 1fr);
    gap: 2rem;
}

/* Premium Page Headers */
.page-header {
    padding: 8rem 0 5rem;
    background: var(--bg-main);
    position: relative;
    overflow: hidden;
}

.page-header::before {
    content: '';
    position: absolute;
    top: -30%;
    right: -10%;
    width: 600px;
    height: 600px;
    background: radial-gradient(circle, rgba(74, 93, 115, 0.06) 0%, transparent 70%);
    border-radius: 50%;
}

.page-header .container {
    position: relative;
    z-index: 2;
    text-align: center;
    max-width: 900px;
}

.page-tag {
    display: inline-block;
    padding: 0.5rem 1.5rem;
    background: linear-gradient(135deg, rgba(74, 93, 115, 0.12) 0%, rgba(74, 93, 115, 0.08) 100%);
    border: 1px solid rgba(74, 93, 115, 0.25);
    border-radius: 30px;
    font-size: 0.8rem;
    font-weight: 700;
    color: var(--accent-it);
    text-transform: uppercase;
    letter-spacing: 0.12em;
    margin-bottom: 1.5rem;
}

.page-header h1 {
    font-size: 4.5rem;
    line-height: 1.1;
    margin-bottom: 1.5rem;
    font-weight: 700;
    letter-spacing: -0.03em;
    background: linear-gradient(135deg, #222222 0%, #2A2A2A 100%);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    background-clip: text;
}

.page-lead {
    font-size: 1.3rem;
    line-height: 1.7;
    color: var(--text-secondary);
    max-width: 750px;
    margin: 0 auto;
}

/* Premium Buttons */
.btn {
    display: inline-block;
    padding: 1rem 2.25rem;
    font-size: 1rem;
    font-weight: 600;
    text-decoration: none;
    border-radius: 12px;
    transition: all 0.3s cubic-bezier(0.25, 1, 0.5, 1);
    cursor: pointer;
    border: none;
    text-align: center;
    letter-spacing: 0.02em;
    position: relative;
    overflow: hidden;
}

.btn::before {
    content: '';
    position: absolute;
    top: 50%;
    left: 50%;
    width: 0;
    height: 0;
    border-radius: 50%;
    background: rgba(255, 255, 255, 0.15);
    transform: translate(-50%, -50%);
    transition: width 0.6s, height 0.6s;
}

.btn:hover::before {
    width: 300px;
    height: 300px;
}

.btn-primary {
    background: linear-gradient(135deg, var(--accent-it) 0%, #3A4D63 100%);
    color: white;
    box-shadow: 0 4px 15px rgba(74, 93, 115, 0.3);
}

.btn-primary:hover {
    transform: translateY(-2px);
    box-shadow: 0 8px 25px rgba(74, 93, 115, 0.4);
}

.btn-outline {
    background: transparent;
    color: var(--accent-it);
    border: 2px solid var(--accent-it);
}

.btn-outline:hover {
    background: var(--accent-it);
    color: white;
    transform: translateY(-2px);
    box-shadow: 0 8px 25px rgba(74, 93, 115, 0.25);
}

/* Text Link Enhancement */
.text-link {
    display: inline-flex;
    align-items: center;
    gap: 0.5rem;
    color: var(--accent-it);
    text-decoration: none;
    font-weight: 600;
    font-size: 1rem;
    transition: all 0.3s ease;
    position: relative;
}

.text-link::after {
    content: '→';
    transition: transform 0.3s ease;
}

.text-link:hover {
    color: #3A4D63;
}

.text-link:hover::after {
    transform: translateX(5px);
}

/* Premium Scrollbar */
::-webkit-scrollbar {
    width: 10px;
    height: 10px;
}

::-webkit-scrollbar-track {
    background: rgba(0, 0, 0, 0.03);
}

::-webkit-scrollbar-thumb {
    background: linear-gradient(135deg, var(--accent-it) 0%, #4A5D73 100%);
    border-radius: 10px;
}

::-webkit-scrollbar-thumb:hover {
    background: linear-gradient(135deg, #3A4D63 0%, #3A4D63 100%);
}

/* Smooth Scroll */
html {
    scroll-behavior: smooth;
}

/* Selection Styling */
::selection {
    background: rgba(74, 93, 115, 0.2);
    color: var(--text-primary);
}

/* Premium Focus States */
*:focus-visible {
    outline: 3px solid rgba(74, 93, 115, 0.4);
    outline-offset: 3px;
    border-radius: 4px;
}

/* Loading State */
@keyframes shimmer {
    0% {
        background-position: -1000px 0;
    }

    100% {
        background-position: 1000px 0;
    }
}

.loading-shimmer {
    background: linear-gradient(90deg, #f0f0f0 25%, #e0e0e0 50%, #f0f0f0 75%);
    background-size: 1000px 100%;
    animation: shimmer 2s infinite;
}

/* Premium Grid Responsive */
@media (max-width: 1024px) {
    .service-cards-grid {
        grid-template-columns: repeat(2, 1fr);
        gap: 1.75rem;
    }

    .page-header h1 {
        font-size: 3.5rem;
    }
}

@media (max-width: 768px) {
    .service-cards-grid {
        grid-template-columns: 1fr;
        gap: 1.5rem;
    }

    .page-header {
        padding: 5rem 0 3.5rem;
    }

    .page-header h1 {
        font-size: 2.75rem;
    }

    .page-lead {
        font-size: 1.15rem;
    }

    .service-card-minimal {
        padding: 2rem 1.75rem;
    }
}

@media (max-width: 480px) {
    .page-header h1 {
        font-size: 2.2rem;
    }

    .page-tag {
        font-size: 0.7rem;
        padding: 0.4rem 1.25rem;
    }

    .service-card-minimal {
        padding: 1.75rem 1.5rem;
    }

    .btn {
        padding: 0.875rem 2rem;
        font-size: 0.95rem;
    }
}

/* Premium Accessibility */
@media (prefers-reduced-motion: reduce) {

    *,
    *::before,
    *::after {
        animation-duration: 0.01ms !important;
        animation-iteration-count: 1 !important;
        transition-duration: 0.01ms !important;
    }
}

/* High Contrast Mode Support */
@media (prefers-contrast: high) {
    .service-card-minimal {
        border: 2px solid currentColor;
    }

    .btn-outline {
        border-width: 3px;
    }
}

/* Dark Mode is now handled by theme-variables.css with [data-theme="dark"] */
/* System preference detection can be added to ThemeContext if needed */


/* GPU Acceleration for Smooth Animations */
.service-card-minimal,
.btn,
.card-arrow,
.text-link {
    will-change: transform;
    transform: translateZ(0);
    backface-visibility: hidden;
}

/* Premium Typography Enhancement */
h1,
h2,
h3,
h4,
h5,
h6 {
    font-feature-settings: 'liga' 1, 'kern' 1;
    text-rendering: optimizeLegibility;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

/* Luxury Gradient Overlays */
.luxury-gradient-overlay {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: linear-gradient(135deg,
            rgba(74, 93, 115, 0.05) 0%,
            rgba(74, 93, 115, 0.03) 50%,
            transparent 100%);
    pointer-events: none;
}

/* Elevated Card Variant */
.service-card-elevated {
    background: linear-gradient(135deg, #E6DDD3 0%, var(--bg-main) 100%);
