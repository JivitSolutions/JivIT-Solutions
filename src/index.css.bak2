/* Responsive styles for various elements */
@media (max-width: 768px) {
    .dropdown-menu {
        position: static;
        box-shadow: none;
        opacity: 1;
        visibility: visible;
        transform: none;
        padding-left: 1rem;
        background: transparent;
        display: none;
    }

    .dropdown-trigger::after {
        content: ' +';
    }

    .login-form-container {
        width: 100%;
        max-width: 100%;
        height: 100vh;
    }
}


/* Animations */
@keyframes fadeInUp {
    from {
        opacity: 0;
        transform: translateY(30px);
    }

    to {
        opacity: 1;
        transform: translateY(0);
    }
}

.hero-content {
    animation: fadeInUp 1.2s ease-out;
}

/* Mobile Nav Enhancements */
@media (max-width: 768px) {
    .site-nav {
        position: fixed;
        top: 0;
        right: -100%;
        width: 80%;
        height: 100vh;
        background: var(--bg-white);
        display: flex;
        flex-direction: column;
        justify-content: center;
        padding: 4rem;
        transition: 0.5s ease;
        box-shadow: -20px 0 60px rgba(0, 0, 0, 0.1);
        z-index: 2000;
    }

    .site-nav.open {
        right: 0;
    }

    .site-nav ul {
        flex-direction: column;
        gap: 2rem;
    }

    .site-nav a {
        font-size: 1.5rem;
    }
}

/* Service Cards - Minimal Design */
.service-cards-grid {
    display: grid;
    grid-template-columns: repeat(auto-fill, minmax(280px, 1fr));
    gap: 2rem;
    margin-top: 3rem;
}

.service-card-minimal {
    background-color: var(--bg-white);
    border: 1px solid rgba(0, 0, 0, 0.06);
    padding: 2.5rem 2rem;
    cursor: pointer;
    transition: all 0.4s cubic-bezier(0.25, 1, 0.5, 1);
    position: relative;
    overflow: hidden;
}

.service-card-minimal::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 3px;
    height: 0;
    background-color: var(--accent-it);
    transition: height 0.4s cubic-bezier(0.25, 1, 0.5, 1);
}

.service-card-minimal:hover {
    transform: translateY(-8px);
    box-shadow: 0 20px 40px rgba(0, 0, 0, 0.08);
    border-color: rgba(0, 0, 0, 0.1);
}

.service-card-minimal:hover::before {
    height: 100%;
}

.service-card-minimal:focus {
    outline: 2px solid var(--accent-it);
    outline-offset: 2px;
}

.service-card-minimal h3 {
    font-size: 1.5rem;
    margin-bottom: 0.75rem;
    color: var(--text-primary);
    font-weight: 500;
    transition: color 0.3s ease;
}

.service-card-minimal:hover h3 {
    color: var(--accent-it);
}

.service-card-minimal p {
    font-size: 0.95rem;
    color: var(--text-secondary);
    margin: 0;
    line-height: 1.6;
}

.card-arrow {
    margin-top: 1.5rem;
    display: flex;
    align-items: center;
    color: var(--accent-it);
    opacity: 0;
    transform: translateX(-10px);
    transition: all 0.3s ease;
}

.service-card-minimal:hover .card-arrow {
    opacity: 1;
    transform: translateX(0);
}

